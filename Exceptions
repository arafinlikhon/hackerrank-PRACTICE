# Input handling
if __name__ == '__main__':
    t = int(input())  # Number of test cases

    for _ in range(t):
        a, b = input().split()  # Input values as strings

        try:
            result = int(a) // int(b)  # Perform integer division
            print(result)
        except ZeroDivisionError as e:
            print("Error Code:", e)
        except ValueError as e:
            print("Error Code:", e)


